import React, { useState, useEffect } from "react";

export const Note = (props) => {
    const [count, setCount] = useState(0);

    useEffect(() => {
        console.log('change count');
    }, [count]);

    return (
        <>
            <p>{count}</p>
            <button onClick={() => setCount(count + 1)}>count up</button>
        </>
    );
}
